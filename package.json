{
  "name": "graasp-app-starter-react",
  "version": "0.1.0",
  "private": true,
  "author": "React EPFL",
  "contributors": [
    "Juan Carlos Farah",
    "Hassan Abdul Ghaffar",
    "Mashkour Abdel Aziz"
  ],
  "homepage": ".",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "4.2.1",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "i18next": "13.1.2",
    "prop-types": "15.6.2",
    "qs": "6.6.0",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-i18next": "9.0.2",
    "react-redux": "6.0.0",
    "react-scripts": "2.1.3",
    "react-select": "2.2.0",
    "reactstrap": "7.0.2",
    "redux": "4.0.1",
    "redux-devtools-extension": "2.13.7",
    "redux-promise": "0.6.0",
    "redux-thunk": "2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn build:react && yarn build:bundle",
    "build:react": "react-scripts build",
    "build:bundle": "webpack --config webpack.config.js --mode=production",
    "build:local": "env-cmd ./.env.local yarn build",
    "build:dev": "env-cmd ./.env.dev yarn build",
    "build:prod": "env-cmd ./.env.prod yarn build",
    "build:version": "REACT_APP_VERSION=$CI_BRANCH env-cmd --no-override ./.env.prod yarn build",
    "lint": "eslint .",
    "test": "react-scripts test",
    "test:ci": "cross-env CI=true env-cmd ./.env.test react-scripts test --env=jsdom --coverage",
    "report": "cat ./coverage/lcov.info | env-cmd ./.env.test codacy-coverage",
    "eject": "react-scripts eject",
    "release": "standard-version"
  },
  "devDependencies": {
    "@commitlint/cli": "7.3.2",
    "@commitlint/config-conventional": "7.3.1",
    "codacy-coverage": "3.4.0",
    "cross-env": "5.2.0",
    "env-cmd": "8.0.2",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.3",
    "glob": "7.1.3",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.3.1",
    "interpolate-html-plugin": "3.0.0",
    "script-ext-html-webpack-plugin": "2.1.3",
    "standard-version": "4.4.0",
    "uglifyjs-webpack-plugin": "2.1.1",
    "webpack-cli": "3.2.1"
  },
  "release": {
    "analyzeCommits": "conventional"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
